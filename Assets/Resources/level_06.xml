<cellcraft>	<levelInfo>		<level index="7" title="Heat Shock Crisis" />		<size width="500" height="500" />		<boundary type="box" width="500" height="500" />		<start x="0" y="0" />		<background name="petridish_silver" />		<resources atp="1500" na="35" aa="1000" fa="100" g="50"/>		<organelles mito="1" chloro="1" ribo="30" lyso="10" slicer="0" perox="3"/>	</levelInfo>	<levelStuff>		<goodies>			<stuff name="na_batch" type="na" count="50" spawn="0.05" active="false" />			<stuff name="fa_batch" type="fa" count="50" spawn="0.1" active="false" />			<stuff name="aa_batch" type="aa" count="100" spawn="0" active="false" />			<stuff name="g_batch" type="g" count="500" spawn="0" active="true" />		</goodies>		<enemies>		</enemies>		<objects>		</objects>	</levelStuff>	<levelThings>		<goodies>			<thing name="Glucose Pool" type="glucose" x="100" y="100" count="50" spawn="1" active="false" />		</goodies>		<waves>			<wave id="time_wave_0" type="virus_injector" count="15" spread="1" vesicle="0" delay="0"/>			<wave id="time_wave_1" type="virus_invader" count="15" spread="1" vesicle="0" delay="0"/>			<wave id="time_wave_2" type="virus_infester" count="15" spread="1" vesicle="0" delay="0"/>						<wave id="inj_0" type="virus_injector" count="30" spread="2" delay="30"/>			<wave id="inv_0" type="virus_invader" count="55" spread="1" delay="30" />		</waves>		<enemies>		</enemies>		<objects>		</objects>	</levelThings>		<levelTriggers>				<objective id="load" active="true" trigger="true">			<data name="Game Loads" hidden="true" type="game_load" targetType="null" targetNum="0" progress="0" targetCondition="true" delay="0" />			<action>				<show_interface id="temperature"/>				<set_temperature value="8"/>				<set_radicals value="true"/>				<set_sunlight value="0.5"/>				<set_heat_change value="-1" time="450"/> 				<add_organelle_act id="mitochondrion" value="toggle_brown_fat"/>			</action>		</objective>				<objective id="start" active="true" trigger="true">			<data name="Game Starts" hidden="true" type="game_start" targetType="null" targetNum="0" progress="0" targetCondition="true" delay="30"/>			<action>				<showTutorial id="Temperature">					<tutorial title="Oh no! Where did the cells go? It's freezing out here!" slide="talk_jeeves_broken"/>					<tutorial title="The insulation in the device will start to fail soon...they'll freeze to death!" slide="talk_jeeves_broken"/>					<tutorial title="My only hope of finding them again is with my thermal scanner..." slide="talk_jeeves_broken"/>					<tutorial title="" slide="about_temp_1"/>					<tutorial title="Hmmm... I've heard BROWN FAT CELLS can burn glucose for heat instead of ATP, I wonder if these cells can do the same..." slide="talk_jeeves_broken"/>					<tutorial title="" slide="about_temp_2"/>					<tutorial title="" slide="newaction_enable_brown_fat"/>					<tutorial title="Burn glucose and reach 25 degrees celsius!" slide="objective" />				</showTutorial>				<activate_objective id="heat_low"/>				<activate_objective id="heat_hi_1"/>				<activate_objective id="heat_hi_2"/>							</action>		</objective>				<objective id="heat_hi_1" active="false" sound="0" trigger="true">			<data name="Reach 10 degrees C!" hidden="true" type="temperature_change" targetType="hot" targetNum="10" progress="0" targetCondition="gr_eq" delay="0"/>			<action>				<showTutorial id="Warning! Viruses!">					<tutorial title="I hope the cells are okay. The virus chamber might have ruptured in the crash..." slide="talk_jeeves_broken"/>					<tutorial title="...that would be bad news. Hope the cells can handle whatever happens." slide="talk_jeeves_broken"/>				</showTutorial>				<activate_objective id="time_0"/>				<start_countdown value="30"/>							</action>		</objective>				<objective id="time_0" active="false" trigger="true">			<data name="countdown" hidden="true" type="countdown" targetType="null" targetNum="0" progress="0" targetCondition="true" delay="15"/>			<action>				<send_wave id="time_wave_0"/>				<activate_objective id="defeat_wave_0"/>			</action>		</objective>				<objective id="defeat_wave_0" active="false" trigger="true">			<data name="Survive the viruses!" hidden="true" type="enemy_survive" targetType="time_wave_0" targetNum="0" progress="0" targetCondition="true" delay="45"/>			<action>				<start_countdown value="60"/>				<activate_objective id="time_1"/>			</action>		</objective>				<objective id="time_1" active="false" trigger="true">			<data name="countdown" hidden="true" type="countdown" targetType="null" targetNum="0" progress="0" targetCondition="true" delay="15"/>			<action>				<send_wave id="time_wave_1"/>				<activate_objective id="defeat_wave_1"/>			</action>		</objective>				<objective id="defeat_wave_1" active="false" trigger="true">			<data name="Survive the viruses!" hidden="true" type="enemy_survive" targetType="time_wave_1" targetNum="0" progress="0" targetCondition="true" delay="45"/>			<action>				<start_countdown value="60"/>				<activate_objective id="time_2"/>			</action>		</objective>				<objective id="time_2" active="false" trigger="true">			<data name="countdown" hidden="true" type="countdown" targetType="null" targetNum="0" progress="0" targetCondition="true" delay="15"/>			<action>				<send_wave id="time_wave_2"/>				<activate_objective id="defeat_wave_2"/>			</action>		</objective>				<objective id="defeat_wave_2" active="false" trigger="true">			<data name="Survive the viruses!" hidden="true" type="enemy_survive" targetType="time_wave_2" targetNum="0" progress="0" targetCondition="true" delay="45"/>			<action>				<start_countdown value="60"/>				<activate_objective id="time_3"/>			</action>		</objective>				<objective id="time_3" active="false" trigger="true">			<data name="countdown" hidden="true" type="countdown" targetType="null" targetNum="0" progress="0" targetCondition="true" delay="15"/>			<action>				<send_wave id="time_wave_3"/>				<activate_objective id="defeat_wave_3"/>			</action>		</objective>				<objective id="heat_low" active="false" sound="0" trigger="true">			<data name="Don't freeze to death!" hidden="false" type="temperature_min" targetType="null" targetNum="0" progress="0" targetCondition="true" delay="0"/>			<action>				<showTutorial>					<tutorial title="Scanner's not picking anything up... they must have frozen to death." slide="talk_jeeves_broken"/>				</showTutorial>				<finish_level value="failure"/>			</action>		</objective>				<objective id="heat_hi_2" active="false" sound="2" trigger="true">			<data name="Reach 25 degrees C!" hidden="false" type="temperature_change" targetType="hot" targetNum="25" progress="0" targetCondition="gr_eq" delay="0"/>			<action>				<showTutorial>					<tutorial title="Let's see... nothing... nothing... nothing..." slide="talk_jeeves_broken"/>					<tutorial title="...wait! Over there!... I'm getting something!" slide="talk_jeeves_broken"/>				</showTutorial>				<finish_level value="victory"/>			</action>		</objective>		</levelTriggers>	</cellcraft>